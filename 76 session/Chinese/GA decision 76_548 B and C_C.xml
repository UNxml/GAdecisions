        <mainBody>
          <paragraph eId="para_1" class="MercuryDocument">
            <content eId="para_1__content">
              <p xml:space="preserve">	2022年6月29日，大会第89次会议根据第五委员会的建议，<authorialNote marker="11" placement="bottom" eId="authorialNote_11"><p xml:space="preserve"> <span class="underline"><a href="https://undocs.org/ch/A/76/634/Add.2">A/76/634/Add.2</a></span>，第7段。</p></authorialNote></p>
            </content>
          </paragraph>
          <paragraph eId="para_2" class="MercuryDocument">
            <content eId="para_2__content">
              <p xml:space="preserve">
                <span class="bold">A 节</span>
              </p>
            </content>
          </paragraph>
          <paragraph eId="para_3" class="MercuryDocument">
            <content eId="para_3__content">
            <p xml:space="preserve">	决定将下列文件推迟到第七十七届会议主要会期审议：</p>
            </content>
          </paragraph>
          <paragraph eId="para_4" class="MercuryDocument">
            <content eId="para_4__content">
              <p xml:space="preserve">项目136</p>
            </content>
          </paragraph>
          <paragraph eId="para_5" class="MercuryDocument">
            <content eId="para_5__content">
              <p xml:space="preserve">审查联合国行政和财政业务效率</p>
            </content>
          </paragraph>
          <paragraph eId="para_6" class="MercuryDocument">
            <content eId="para_6__content">
              <p xml:space="preserve">投资于预防和建设和平</p>
            </content>
          </paragraph>
          <paragraph eId="para_7" class="MercuryDocument">
            <content eId="para_7__content">
              <p xml:space="preserve">	秘书长关于投资于预防和建设和平的报告<authorialNote marker="12" placement="bottom" eId="authorialNote_12"><p xml:space="preserve"> <span class="underline"><a href="https://undocs.org/ch/A/76/732">A/76/732</a></span>。</p></authorialNote></p>
            </content>
          </paragraph>
          <paragraph eId="para_8" class="MercuryDocument">
            <content eId="para_8__content">
              <p xml:space="preserve">	行政和预算问题咨询委员会的相关报告<authorialNote marker="13" placement="bottom" eId="authorialNote_13"><p xml:space="preserve"> <span class="underline"><a href="https://undocs.org/ch/A/76/821">A/76/821</a></span>。</p></authorialNote></p>
            </content>
          </paragraph>
          <paragraph eId="para_9" class="MercuryDocument">
            <content eId="para_9__content">
              <p xml:space="preserve">
                <span class="bold">B 节</span>
              </p>
            </content>
          </paragraph>
          <paragraph eId="para_10" class="MercuryDocument">
            <content eId="para_10__content">
            <p xml:space="preserve">	决定将下列文件推迟到第七十七届会议续会第二期会议审议：</p>
            </content>
          </paragraph>
          <paragraph eId="para_11" class="MercuryDocument">
            <content eId="para_11__content">
              <p xml:space="preserve">目153</p>
            </content>
          </paragraph>
          <paragraph eId="para_12" class="MercuryDocument">
            <content eId="para_12__content">
              <p xml:space="preserve">联合国科特迪瓦行动经费的筹措</p>
            </content>
          </paragraph>
          <paragraph eId="para_13" class="MercuryDocument">
