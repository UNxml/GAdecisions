    <preface>
      <p>
        <docNumber refersTo="decisionNumber">76/407.</docNumber>
        <docTitle refersTo="#decisionTitle">
          <span class="bold">Nombramiento de miembros de la Comisión Consultiva en Asuntos Administrativos y de Presupuesto</span>
        </docTitle>
      </p>
      <p>
        <docNumber refersTo="#letter">
          <span class="bold">B</span>
