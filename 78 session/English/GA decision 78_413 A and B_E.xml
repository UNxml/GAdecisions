              <FRBRthis value="/akn/un/statement/deliberation/unga/2024-03-21/78-413B/eng@!main/"></FRBRthis>
              <FRBRuri value="/akn/un/statement/deliberation/unga/2024-03-21/78-413B/eng@/"></FRBRuri>
              <FRBRdate date="2024-04-30" name="publication"></FRBRdate>
