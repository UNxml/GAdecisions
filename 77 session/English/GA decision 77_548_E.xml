      <paragraph eId="para_1" class="MercuryDocument">
        <content eId="para_1__content">
          <p xml:space="preserve">	At its 56th (resumed) plenary meeting, on 30 December 2022, the General Assembly, on the recommendation of the Fifth Committee,<authorialNote marker="1" placement="bottom" eId="authorialNote_1"><p xml:space="preserve"> <a href="https://undocs.org/en/A/77/673">A/77/673</a>, para. 11.</p></authorialNote></p>
        </content>
      </paragraph>
      <paragraph eId="para_2" class="MercuryDocument">
        <content eId="para_2__content">
          <p xml:space="preserve">
            <span class="bold">Section A </span>
          </p>
        </content>
      </paragraph>
      <paragraph eId="para_3" class="MercuryDocument">
        <content eId="para_3__content">
          <p xml:space="preserve"> Decided to defer to the first part of its resumed seventh-seventh session consideration of the following documents: </p>
        </content>
      </paragraph>
      <paragraph eId="para_4" class="MercuryDocument">
        <content eId="para_4__content">
          <p xml:space="preserve">
            <span class="italic">Item 136 </span>
          </p>
        </content>
      </paragraph>
      <paragraph eId="para_5" class="MercuryDocument">
        <content eId="para_5__content">
          <p xml:space="preserve">
            <span class="italic">Review of the efficiency of the administrative and financial functioning of the United Nations </span>
          </p>
        </content>
      </paragraph>
      <paragraph eId="para_6" class="MercuryDocument">
        <content eId="para_6__content">
          <p xml:space="preserve">
            <span class="italic">Investing in prevention and peacebuilding</span> </p>
        </content>
      </paragraph>
